<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">

 <fieldset>
    <legend>{{ "registration.title.account" |trans }}</legend>
    
    {{ form_widget(form) }}
    
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
        
  </fieldset>

 <fieldset>
    <legend>{{ "registration.title.company" |trans }}</legend>
  </fieldset>

 <fieldset>
    <legend>{{ "registration.title.store" |trans }}</legend>
  </fieldset>


</form>
